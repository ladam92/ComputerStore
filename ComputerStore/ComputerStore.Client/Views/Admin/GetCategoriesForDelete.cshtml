@model List<ComputerStore.DTO.Types.AlkatreszTipus>

<div class="col-md-4 col-md-offset-4">

    <table class="table table-hover table-bordered centerText">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Megnevezes
                    <a href="@Url.Action("DeleteCategoryConcrete", "Admin", new { id = item.ID, type = (int)item.Tipus })"><span class="glyphicon glyphicon-trash pull-right" aria-hidden="true"></span></a>
                </td>
                
            </tr>
        }
    </table>

</div>